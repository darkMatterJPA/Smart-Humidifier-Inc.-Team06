<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">


    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="140dp"
        android:layout_height="47dp"
        android:layout_marginTop="448dp"
        android:layout_marginEnd="252dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TimePicker
        android:id="@+id/startTimePicker"
        android:layout_width="169dp"
        android:layout_height="110dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="224dp"
        android:timePickerMode="spinner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/sched"
        android:layout_width="197dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:text="SCHEDULE"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/Bluepurple"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/tM"
        style="@style/toggleButton"
        android:layout_width="58dp"
        android:layout_height="55dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="300dp"
        android:background="@drawable/toggle_bg"
        android:textOff="M"
        android:textOn="M"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.272"
        app:layout_constraintStart_toEndOf="@+id/tSu"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/tW"
        style="@style/toggleButton"
        android:layout_width="58dp"
        android:layout_height="55dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="188dp"
        android:background="@drawable/toggle_bg"
        android:textOff="W"
        android:textOn="W"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tT"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/tTh"
        style="@style/toggleButton"
        android:layout_width="57dp"
        android:layout_height="55dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="132dp"
        android:background="@drawable/toggle_bg"
        android:textOff="Th"
        android:textOn="Th"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.062"
        app:layout_constraintStart_toEndOf="@+id/tW"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/tSu"
        style="@style/toggleButton"
        android:layout_width="58dp"
        android:layout_height="55dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="352dp"
        android:background="@drawable/toggle_bg"
        android:textOff="Su"
        android:textOn="Su"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/tT"
        style="@style/toggleButton"
        android:layout_width="57dp"
        android:layout_height="55dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="244dp"
        android:background="@drawable/toggle_bg"
        android:textOff="T"
        android:textOn="T"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tM"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/tF"
        style="@style/toggleButton"
        android:layout_width="58dp"
        android:layout_height="55dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="76dp"
        android:background="@drawable/toggle_bg"
        android:textOff="F"
        android:textOn="F"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tTh"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/tS"
        style="@style/toggleButton"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/toggle_bg"
        android:textOff="S"
        android:textOn="S"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tF"
        app:layout_constraintTop_toTopOf="parent" />

    <TimePicker
        android:id="@+id/endTimePicker"
        android:layout_width="169dp"
        android:layout_height="110dp"
        android:layout_marginTop="280dp"
        android:layout_marginEnd="224dp"
        android:timePickerMode="spinner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/startTimeTF"
        android:layout_width="201dp"
        android:layout_height="51dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="204dp"
        android:text="START TIME"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/Bluepurple"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/endTimeTF"
        android:layout_width="201dp"
        android:layout_height="51dp"
        android:layout_marginTop="252dp"
        android:layout_marginEnd="204dp"
        android:text="END TIME"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/Bluepurple"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/save"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="172dp"
        android:layout_marginEnd="44dp"
        android:backgroundTint="@color/Bluepurple"
        android:text="SAVE "
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="300dp"
        android:layout_marginEnd="44dp"
        android:backgroundTint="@color/Bluepurple"
        android:text="cancel"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/humidLevel"
        android:layout_width="201dp"
        android:layout_height="51dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="396dp"
        android:text="HUMIDITY LEVEL"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/Bluepurple"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>